{
    "name": "allesonline/ingrado-website",
    "type": "project",
    "description": "Ingrado Website",
    "keywords": [
        "ingrado",
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "ext-json": "*",
        "ext-simplexml": "*",
        "ao/cms": "2.17.x-dev",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/sanctum": "^3.3",
        "laravel/tinker": "^2.8",
        "league/flysystem-aws-s3-v3": "^3.15.0",
        "symfony/http-client": "^7.0",
        "symfony/mailgun-mailer": "^7.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.20.0",
        "laravel/pint": "^1.18",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/collision": "^7.0",
        "pestphp/pest": "^2.35",
        "pestphp/pest-plugin-laravel": "^2.4",
        "spatie/laravel-ignition": "^2.0"
    },
    "repositories": [
        {
            "type": "git",
            "url": "git@git.allesonline.nl:allesonline/package-cms.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "pint": "./vendor/bin/pint",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi",
            "@php artisan vendor:publish --tag=admin --force",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-update-cmd": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan module:ide"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": false,
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
